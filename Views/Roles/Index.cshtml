@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Index";
}
<div><a asp-action="Create">Create Role</a></div>
<h2>List of Roles </h2>
<table class="table table-striped table-bordered">
    <tr>
        <td>ID</td>
        <td>Name</td>
    </tr>

    @foreach (var role in Model)
    {
        <tr>
            <td>@role.Id</td>
            <td>@role.Name</td>
        </tr>
    }

</table>


