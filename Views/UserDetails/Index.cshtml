@model IEnumerable<ProjectYogaMed.Models.UserDetails>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Useremail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usercontact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dob)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Useremail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usercontact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dob)
            </td>
           
            <td>
                <a asp-action="Edit" class="btn btn-primary"asp-route-id="@item.UserId">Edit</a> |
                <a asp-action="Details"class="btn btn-primary" asp-route-id="@item.UserId">Details</a> |
                <a asp-action="Delete"class="btn btn-primary" asp-route-id="@item.UserId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<div>
    <partial name="DView" />
</div>
